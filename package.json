{
  "name": "@botmonio/sdk",
  "version": "0.7.0",
  "description": "BotMon SDK for Cloudflare Workers - Track analytics events with batching and retry logic",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node esbuild.config.js && tsc --emitDeclarationOnly",
    "dev": "node esbuild.config.js --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "botmon",
    "cloudflare",
    "workers",
    "analytics",
    "bot-detection",
    "traffic-analytics",
    "web-analytics"
  ],
  "author": "BotMon <hello@botmon.io>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/Botmonio/botmon-sdk"
  },
  "homepage": "https://github.com/Botmonio/botmon-sdk",
  "bugs": {
    "url": "https://github.com/Botmonio/botmon-sdk/issues"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "esbuild": "^0.20.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
